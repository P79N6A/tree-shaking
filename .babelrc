{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "browsers": [
                        ">1%",
                        "last 3 versions",
                        "Firefox ESR",
                        "not ie < 9"
                    ]
                },
                "modules": false,
                "useBuiltIns": "entry",
            }
        ],
        "@babel/preset-react",
    ],
    "plugins": [
        [
            "@babel/plugin-proposal-decorators",
            {
                "legacy": true
            }
        ],
        "@babel/plugin-proposal-class-properties",
    ]
}